<!-- Shopex main column begin -->
<section id="main-content" class="col-md-9 col-sm-12 col-xs-12">
  <!-- main content here -->              
    <div class="row">
      <!-- title begin -->
      <div class="title-wrap hidden-xs">
        <div class="col-md-6 col-sm-6 col-xs-6">
          <ul class="nav navbar-nav nav-tabs-google">
            <li><a href="/shopping-experiences">ALL SHOPPING EXPERIENCES</a></li>
          </ul>
        </div>
        <div class="col-md-6 col-sm-6 col-xs-6 ">
          <ul class="nav navbar-nav navbar-right  nav-tabs-google">
            <li class="<%= list_order == "recent" ? "active" : "" %>"><a href="/shopping-experiences/recent">Recent</a></li>
            <li class="<%= list_order == "topvoted" ? "active" : "" %>"><a href="/shopping-experiences/topvoted">Top Voted</a></li>
          </ul>
        </div>
      </div>
      <!-- title end -->
    </div>

    <section id="content" class="shopex_short_list" order="<%= list_order %>">
      <%- rendered_include('views/zs_shopex_short_list.ejs', {shopex_list_data: shopex_main_column_data.shopex_list_data, 
        user_info: user_info,
        loggedIn: loggedIn,
        }) %>
    </section>

    <div class="content-wrap">
      <button class="col-md-12 shopex-short-load-more">Load More Shopping Eperiences</button>        
    </div>

</section>

<!-- Shopex main column end -->

<aside id="zs-col-right">   
  <div class="col-md-3 hidden-sm hidden-xs">

    <div style="height:20px"></div>

    <!-- <div class="zs-help">
      <div class="well well-sm">
        <div class="help-heading" style="text-align: center;">
          <span>
            Know About Us
          </span>
        </div>
        <br>
        <div class="help-action" style="text-align: center;">
          <a href="/about-zorsay" target="_blank" class="zs-defaultbtn">About us</a>
        </div>
      </div>
    </div> -->

    <div class="zs-luv">
      <div class="well well-sm">
        <div class="luv-heading zs-share-heading">
          <span>
            <% if (typeof(page_info) != "undefined")  { %>
              Share This <i><a href="<%=page_info.page_link%>"target="_blank"><%= page_info.post_type %></a></i>
            <% } else { %>
              Love Zorsay !!
            <% } %>
          </span>
        </div>                    
        <div class="luv-action">

          <% if (typeof(page_info) != "undefined")  { %>
            <a class="zs_fb_share zs_social_share" href="<%=fb_share(page_info)%>"target="_blank"><img src="/img/fb.png" alt="Share This <%= page_info.post_type %> on Facebook"></a>
            <a class="zs_tw_share zs_social_share" href="<%=tw_share(page_info)%>"target="_blank"><img src="/img/twitter.png" alt="Share This <%= page_info.post_type %> on Twitter"></a>
            <a class="zs_gp_share zs_social_share" href="<%=gp_share(page_info)%>"target="_blank"><img src="/img/google.png" alt="Share This <%= page_info.post_type %> on Google+"></a>
          <% } else { %>
            <a href="https://www.facebook.com/say.zorsay"target="_blank"><img src="/img/fb.png" alt="Zorsay.com Facebook Page"></a>
            <a href="https://twitter.com/sayZorsay"target="_blank"><img src="/img/twitter.png" alt="Zorsay.com Facebook Page"></a>
            <a href="https://plus.google.com/+Zorsay"target="_blank"><img src="/img/google.png" alt="Zorsay.com Facebook Page"></a>
          <% } %>

        </div>
      </div>
    </div>
    <hr>

    <% if(side_bar_data) { %>

      <% if(side_bar_data.sellers && side_bar_data.sellers.length > 0) { %>
        <div class="zs-rel-seller">
          <div class="well well-sm">
            <div class="rel-seller-heading">
              <span>
                Sellers
              </span>
            </div>                    
            <div class="rel-seller-tag">
              <% for(var i = 0; i < side_bar_data.sellers.length; ++i ) { %>
                <a target="_blank" href="/seller/<%= side_bar_data.sellers[i]._id %>/<%= side_bar_data.sellers[i].url_name %>" data-poload="/seller/po/<%= side_bar_data.sellers[i]._id %>" data-uv-triggerx="smartvote">
                  <span class="label label-seller">
                    <%= side_bar_data.sellers[i].name %>
                  </span>
                </a>
              <% } %>
            </div>
          </div>
        </div>
      <% } %>

      <% if(side_bar_data.procats && side_bar_data.procats.length > 0) { %>
        <div class="zs-rel-ps-cat">
          <div class="well well-sm">
            <div class="rel-ps-cat-heading">
              <span>
                Product Categories
              </span>
            </div>                    
            <div class="rel-ps-category-tag">
              <% for(var i = 0; i < side_bar_data.procats.length; ++i ) { %>
                <a target="_blank" href="/product-category/<%= side_bar_data.procats[i]._id %>/<%= side_bar_data.procats[i].url_name %>" data-poload="/product-category/po/<%= side_bar_data.procats[i]._id %>" data-uv-triggerx="smartvote">
                  <span class="label label-ps-category">
                    <%= side_bar_data.procats[i].name %>
                  </span>
                </a>
              <% } %>
            </div>
          </div>
        </div>
      <% } %>

      <% if(side_bar_data.sercats && side_bar_data.sercats.length > 0) { %>
        <div class="zs-rel-ps-cat">
          <div class="well well-sm">
            <div class="rel-ps-cat-heading">
              <span>
                Service Categories
              </span>
            </div>                    
            <div class="rel-ps-category-tag">
              <% for(var i = 0; i < side_bar_data.sercats.length; ++i ) { %>
                <a target="_blank" href="/service-category/<%= side_bar_data.sercats[i]._id %>/<%= side_bar_data.sercats[i].url_name %>" data-poload="/service-category/po/<%= side_bar_data.sercats[i]._id %>" data-uv-triggerx="smartvote">
                  <span class="label label-ser-category">
                    <%= side_bar_data.sercats[i].name %>
                  </span>
                </a>
              <% } %>
            </div>
          </div>
        </div>
      <% } %>

      <% if(side_bar_data.questions) { %>
        <div class="zs-side-questions">
          <div class="well well-sm">
            <div class="zs-side-heading">
              <span>More Questions</span>
            </div> 
            <div class="zs-side-content">
              <ul class="list-unstyled">
                <% for(var i = 0; i < side_bar_data.questions.length; ++i ) { %>
                  <li>
                    <a target="_blank" href="<%= side_bar_data.questions[i].permalink %>"><%= side_bar_data.questions[i].content_title %></a>
                  </li>
                <% } %>   
              </ul>
            </div>                      
          </div>              
        </div>
      <% } %>

      <% if(side_bar_data.shopexes) { %>
        <div class="zs-side-shopexes">
          <div class="well well-sm">
            <div class="zs-side-heading">
              <span>More Shopping Experiences
            </span>
            </div> 
            <div class="zs-side-content">
              <ul class="list-unstyled">
                <% for(var i = 0; i < side_bar_data.shopexes.length; ++i ) { %>
                  <li>
                    <a target="_blank" href="<%= side_bar_data.shopexes[i].permalink %>"><%= side_bar_data.shopexes[i].content_title %></a>
                  </li>
                <% } %>
              </ul>                      
            </div>                      
          </div>              
        </div>
      <% } %>

    <% } %>

  </div>
</aside>   

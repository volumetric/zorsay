<div class="row">
  <div class="col-md-12 col-sm-12 col-xs-12">
    <section id="usr-panel">            
        <div class="usr-navwrap">

          <div class="col-md-2 col-sm-4 col-xs-6">
            <div class="usr-pic">
              <div class="usr_profile_img_container">
                <img src="
                <% if(!main_data.user_account.profile_image_url) { %>
                  /img/newuser.jpg
                <% } else { %>
                  <%= main_data.user_account.profile_image_url %>
                <% } %>
                " alt="<%= main_data.user_account.name.first %> <%= main_data.user_account.name.last %>" id="user_profile_img_preview">
                <img src="" style="display: none;" id="user_icon_img_preview">
                <!-- <canvas id="image_preprocess_canvas"> -->
              </div>
              <!-- <span id="user_profile_img_edit"><a href="#"> Edit</a></span> -->
              <a href="#"><p id="user_profile_img_edit_link">Edit Photo</p></a>
              <p id="user_profile_img_status"></p>
              <input class="hidden" type="file" id="user_profile_img_edit" accept="image/*"/>
              
              <div src="<%= main_data.user_account.profile_image_url %>" id="user_profile_img_original" style="display:none;"></div>
              <!-- <input type="hidden" id="user_profile_img_avatar_url" name="avatar_url" value="/public/images/default.png" /> -->
              
              <a href="#" id="user_profile_img_submit" style="display:none;">Submit </a> 
              <a href="#" id="user_profile_img_cancel" style="display:none;">Cancel</a>
            </div>
          </div>

          <div class="col-md-6 col-sm-4 col-xs-2">
            <div class="usr-info">
              <!-- <a href="/user/123/vinit-agrawal-2"> -->
              <a <% if(hasValue(main_data.user_account.permalink)) { %>
                href="<%= main_data.user_account.permalink %>"
              <% } %> >
              
              <h1><%= main_data.user_account.fullname %></h1>
              </a>
              <!-- <span id="user_profile_edit"><a href="#"> Edit</a></span> -->
              <p>
                <span class="text-muted">
                  <% if(hasValue(main_data.user_account.email)) { %>
                    <%= main_data.user_account.email %>
                  <% } %>
                </span>
              </p>

              <p><b>
                <span class="text-muted">
                  <% if(hasValue(main_data.user_account.short_bio)) { %>
                    <%= main_data.user_account.short_bio %>
                  <% } %>
                </span>
              </b></p>

              <p>
                <span class="text-muted">
                  <% if(hasValue(main_data.user_account.aboutme)) { %>
                    <%= main_data.user_account.aboutme %>
                  <% } %>
                </span>
              </p>

              <div class="">
                <a>
                  <% if(hasValue(main_data.user_account.meta_info.followers)) { %>
                    <%= main_data.user_account.meta_info.followers.length %>
                  <% } else { %>
                    0
                  <% } %>
                </a>
                <span>Followers</span>
              </div>

            </div>                    
          </div>

          <!-- <div class="col-md-4 col-sm-4 col-xs-4">
            <div class="usr-follow-stat">
              <div data-toggle="buttons">
                <div class="mini-followers">
                  <a>
                    <% if(hasValue(main_data.user_account._followers)) { %>
                      <%= main_data.user_account.followers.length %>
                    <% } else { %>
                      0
                    <% } %>
                  </a>
                  <span>Followers</span>
                </div> 
                <label class="btn btn-follow pull-right"><input type="checkbox">Follow</label>
              </div>
            </div>
          </div> -->

        </div>
    </section>
  </div>
</div>

<!-- User Profile Section full page - Begin-->
<section id="Account_<%= main_data.user_account._id %>" model="account" num="<%= main_data.user_account._id %>" class="zs-div entity-div account-div">
  <div class="usrinfo">
    <div class="col-md-3 col-sm-3 hidden-xs">
      <div class="usrinfo-sidebar " role="complementary">
        <ul class="nav list-group">                
          <a href="#" class="list-group-item active" value="about">
            About
          </a>
          <a href="#" class="list-group-item hidden" value="questions">
            <span class="badge badge-count">
              <% if(hasValue(main_data.user_account._questions)) { %>
                <%= main_data.user_account._questions.length %>
              <% } else { %>
                0
              <% } %>
            </span>
            Questions
          </a>
          <a href="#" class="list-group-item hidden" value="answers">
            <span class="badge badge-count">
              <% if(hasValue(main_data.user_account._answers)) { %>
                <%= main_data.user_account._answers.length %>
              <% } else { %>
                0
              <% } %>
            </span>
            Answers
          </a>
          <a href="#" class="list-group-item hidden" value="shopexes">
            <span class="badge badge-count">
              <% if(hasValue(main_data.user_account._shopexes)) { %>
                <%= main_data.user_account._shopexes.length %>
              <% } else { %>
                0
              <% } %>
            </span>
            Experiences
          </a>
          <a href="#" class="list-group-item" value="settings">
            Settings
          </a>
          <a href="#" class="list-group-item" value="edit">
            Edit
          </a>
          <a href="#" class="list-group-item" value="change-password">
            Change Password
          </a>
        </ul>              
      </div>            
      </div>

      <!-- Main usrinfo content Start -->
      <div class="col-md-9 col-sm-9 col-xs-12 usrinfo-display about zs-populated">
        <!-- <h4>Basic Information</h4> -->
        <table class="table table-condensed usr-profile">
          <h4>Account Information</h4>
          <tbody>
            <tr>
              <td class="col-xs-2 usr-param">Name</td>
              <td class="usr-data">
                <%= main_data.user_account.fullname %>
              </td>
            </tr>

            <!-- <tr>
              <td class="col-xs-2 usr-param">AGEnder</td>
              <td class="usr-data">
                <%= main_data.user_account.age %> <%= main_data.user_account.gender %>
              </td>
            </tr> -->
            

            <%// console.log("mmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmm") %>
            <% if(hasValue(main_data.user_account.created_at)) { %>
              <tr>
                <td class="col-xs-1 usr-param">Joined On</td>
                <td class="usr-data">
                  <%= main_data.user_account.created_at %>
                  <%//= typeof(main_data.user_account.created_at) %>
                </td>
              </tr>
            <% } %>

            <tr>
              <td class="col-xs-1 usr-param">Short Bio</td>
              <td class="usr-data">
                  <%= main_data.user_account.short_bio ? main_data.user_account.short_bio : "N/A" %>
              </td>
            </tr>

            <tr>
              <td class="col-xs-1 usr-param">Big Bio</td>
              <td class="usr-data">
                  <%= main_data.user_account.aboutme ? main_data.user_account.aboutme : "N/A" %>
              </td>
            </tr>

            <tr>
              <td class="col-xs-1 usr-param">Area Code</td>
              <td class="usr-data">
                <%= main_data.user_account.areacode ? main_data.user_account.areacode : "N/A" %>
              </td>                    
            </tr>

            <tr>
              <td class="col-xs-1 usr-param">Email</td>
              <td class="usr-data"><%= main_data.user_account.email %></td>
              <% if(hasValue(main_data.user_account.verified) && !main_data.user_account.verified) { %>
                <td class="usr-data">
                  <a class="verify_email" href="#">verify email</a>
                </td>
              <% } %>
            </tr>
            <!-- <tr>
              <td class="col-xs-1 usr-param">Password</td>
              <td class="usr-data"><a href="">Reset Password</a></td>                    
            </tr> -->
          </tbody>
          </table>
          
        </div>
        <div class="col-md-9 col-sm-9 col-xs-12 usrinfo-display questions" style="display:none;"></div>
        <div class="col-md-9 col-sm-9 col-xs-12 usrinfo-display answers" style="display:none;"></div>
        <div class="col-md-9 col-sm-9 col-xs-12 usrinfo-display shopexes" style="display:none;"></div>
        <div class="col-md-9 col-sm-9 col-xs-12 usrinfo-display settings" style="display:none;"></div>
        <div class="col-md-9 col-sm-9 col-xs-12 usrinfo-display edit" style="display:none;"></div>
        <div class="col-md-9 col-sm-9 col-xs-12 usrinfo-display change-password" style="display:none;"></div>
      <!-- Main usrinfo content End -->
  </div>
</section>
<!-- User Profile Section full page - End-->
